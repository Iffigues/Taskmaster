[numprocs]
commande = ../test/test
numprocs = 2


[autostart]
commande = ../test/test
autostart= yes
args="loop"

[autorestart_ever]
commande = ../test/test
args="loop"
autorestart=ever

[autorestart_never]
commande = ../test/test
args="loop"
autorestart=never

[autorestart_unexpected]
commande = ../test/test
args="loop 3"
autorestart=unexpected
exitcodes="6"

[exitcodes_true]
commande = ../test/test
args="loop 3"
autorestart="3 4"

[exitcodes_false]
commande = ../test/test
args="loop 6"
autorestart="3 4"

[starttime_true]
commande = ../test/test
args="time 10"
starttime= 5

[starttime_false]
commande = ../test/test
args="time 1"
astartime="50"

[stopsignal_true]
commande = ../test/test
args="signal TERM"
stopsignal="TREM"

[stopsignal_false]
commande = ../test/test
args="signal TERM"
stopsignal="HUP"

[stoptime_true]
commande = ../test/test
args="signal TERM"
stopsignal="TERM"
stoptime=10

[stoptime_false]
commande = ../test/test
args="signal TERM"
stopsignal="HUP""
stoptime=10

[stderr_stdout_redirect]
commande = ../test/test
args="stdout stderr"
stderr="../stderr"
stdout="../stdout"

[stderr_stdout_none]
commande = ../test/test
args="stdout stderr"
stderr=none
stdout=none

[stderr_stdout_default]
commande = ../test/test
args="stdout stderr"


[stderr_stdout_true]
commande = ../test/test
dir="../"
